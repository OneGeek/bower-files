{
  "name": "bower-files",
  "version": "2.0.0",
  "description": "Pulls in dynamic list of filepaths to bower components",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha",
    "codeclimate": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codeclimate && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ksmithut/bower-files.git"
  },
  "keywords": [
    "bower",
    "task management"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/bower-files/issues"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.2",
    "jshint-stylish": "^1.0.0",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "^0.0.1"
  },
  "dependencies": {
    "defaults": "^1.0.0",
    "glob": "^4.0.6",
    "is-relative": "^0.1.1",
    "merge": "^1.2.0"
  }
}
