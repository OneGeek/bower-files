{
  "name": "bower-files",
  "version": "3.13.3",
  "main": "./index",
  "description": "Pulls in dynamic list of filepaths to bower components",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "bin": {
    "bower-files": "bin/bower-files"
  },
  "scripts": {
    "pretest": "npm run test:lint",
    "test": "npm run test:unit",
    "posttest": "npm run test:coverage",
    "test:lint": "eslint . --ignore-path=.gitignore",
    "test:unit": "NODE_ENV=test istanbul cover _mocha",
    "test:coverage": "istanbul check-coverage",
    "test:coverage:report": "open coverage/lcov-report/index.html",
    "test:coverage:codeclimate": "cat ./coverage/lcov.info | codeclimate-test-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ksmithut/bower-files.git"
  },
  "keywords": [
    "bower",
    "task management"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/bower-files/issues"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.2.0",
    "eslint-config-ksmithut": "^2.1.3",
    "eslint-plugin-mocha": "^4.2.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "dependencies": {
    "bower-config": "^1.4.0",
    "debug": "^2.2.0",
    "globby": "^6.0.0",
    "immutable": "^3.8.1",
    "minimatch": "^3.0.2",
    "untildify": "^3.0.2"
  }
}
